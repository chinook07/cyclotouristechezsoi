<IfModule mod_rewrite.c>

    RewriteEngine On
    RewriteBase /
    RewriteRule ^index\.html$ - [L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteCond %{REQUEST_FILENAME} !-l
    RewriteRule . /index.html [L]
    RewriteCond %{HTTP_HOST} ^www\.cyclotouristechezsoi\.ca$ [NC]
    RewriteRule ^(.*)$ https://cyclotouristechezsoi.ca/$1 [R=301,L]

</IfModule>